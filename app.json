{
  "expo": {
    "name": "cedi",
    "slug": "cedi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cedi",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-cedi-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e8d9ca"
    },
    "ios": {
      "bundleIdentifier": "com.soycedi.cedi",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "package": "com.soycedi.cedi",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#e8d9ca"
      },
      "useNextNotificationsApi": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-cedi-splash.png",
          "imageWidth": 800,
          "resizeMode": "contain",
          "backgroundColor": "#e8d9ca"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/ClashDisplay-Regular.otf"
          ]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#ffffff",
          "sounds": ["./assets/sounds/notification.wav"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "90a7579b-5a8d-4191-8bba-bc4f00537579"
      }
    },
    "owner": "cedi0s"
  }
}
